{% extends "layout.njk" %}

{% block content %}
<main class="container">
    <h1>{{ title }}</h1>
    <p>{{ description }}</p>
    <ul>
        {% for character in characters %}
            <li>
                Name: {{ character.name }}
            </li>
        {% else %}
            <li>No characters found.</li>
        {% endfor %}
    </ul>
</main>
{% endblock %}